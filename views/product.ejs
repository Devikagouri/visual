      <script>
        async function showImageDetails(){
              const product_details = JSON.parse(localStorage.getItem('productDetails'))
              console.log(product_details)

              var imageElement = document.createElement('img')
              imageElement.src = `data:${product_details.document_key.image.contentType};base64,${product_details.document_key.image.data.toString('base64')}`;
              imageElement.height = product_details.document_key.height*2
              imageElement.width = product_details.document_key.width*2
              imageElement.class = "img-fluid medium-zoom-image"
              document.getElementById('imageSlot').appendChild(imageElement)

              document.getElementById('titleSlot').innerHTML = product_details.document_key.title
              document.getElementById('artistSlot').innerHTML = product_details.user_key.name
              document.getElementById('descriptionSlot').innerHTML = product_details.document_key.description
              document.getElementById('themeSlot').innerHTML = product_details.document_key.theme
              document.getElementById('heightSlot').innerHTML = product_details.document_key.height
              document.getElementById('widthSlot').innerHTML = product_details.document_key.width
          }         
          
        async function newQuote(){
            var formdata = new FormData();

            var token = localStorage.getItem('token');
            var current_image = JSON.parse(localStorage.getItem('productDetails'))
            
            
            var image = current_image.document_key.image;
            var title = current_image.document_key.title;
            var image_id = current_image.document_key._id;

            console.log(title)
            console.log(image_id)
            console.log(image)

            var price = document.getElementById('quoteAmount').value
            
           const arr = {token,title,image_id,price}
           const obj = JSON.stringify(arr)


            fetch('/quote',{
                method: 'POST',
                headers: {
                    'Content-type': 'application/json'
                },
                body: obj
            }).then(response=>{
                if(response.ok){
                    window.confirm("Quoted Successfully")
                }
                else{
                    window.confirm("Failed to Quote")
                }
            }).catch(error=>{
                console.log(error);
            })
        }
    </script>
  
  
  
  
  
  
