      <script>
          async function showImageDetails(){
                const product_details = JSON.parse(localStorage.getItem('productDetails'))
                console.log(product_details)

                var imageElement = document.createElement('img')
				imageElement.src = `data:${product_details.document_key.image.contentType};base64,${product_details.document_key.image.data.toString('base64')}`;
                imageElement.height = product_details.document_key.height
                imageElement.width = product_details.document_key.width
                imageElement.class = "img-fluid medium-zoom-image"
                document.getElementById('imageSlot').appendChild(imageElement)

                document.getElementById('titleSlot').innerHTML = product_details.document_key.title
                document.getElementById('descriptionSlot').innerHTML = product_details.document_key.description
                document.getElementById('themeSlot').innerHTML = product_details.document_key.theme
                document.getElementById('heightSlot').innerHTML = product_details.document_key.height
                document.getElementById('widthSlot').innerHTML = product_details.document_key.width
            }          
      </script>
  
  
  
  
  
